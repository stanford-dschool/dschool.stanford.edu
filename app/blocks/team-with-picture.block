<div class="TeamMembers" data-team-members>
  {.repeated section items}
    <div class="TeamMember" data-team-member>
      {.main-image?}
        <a href="#" class="TeamMember-picture">
          <img src="{assetUrl}" />
        </a>
      {.end}
      <div class="TeamMember-name">
        {title}
      </div>
      <div class="TeamMember-title">
        {excerpt}
      </div>
      <div class="TeamMember-details" aria-hidden="true">
        {body}
      </div>
    </div>
  {.end}
</div>
